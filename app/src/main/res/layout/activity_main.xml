<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="medart.mygoodproject.MainActivity">


    <TextView
        android:text="UserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="61dp"
        android:id="@+id/textView17" />

    <TextView
        android:text="Email Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:layout_marginTop="32dp"
        android:id="@+id/textView19" />

    <TextView
        android:text="Mobile No."
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="40dp"
        android:id="@+id/textView20"
        android:layout_alignLeft="@+id/textView19"
        android:layout_alignStart="@+id/textView19"
        android:layout_toLeftOf="@+id/editText12"
        android:layout_toStartOf="@+id/editText12" />

    <TextView
        android:text="Sim Serial No."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/textView20"
        android:layout_alignEnd="@+id/textView20"
        android:id="@+id/textView21"
        android:layout_alignLeft="@+id/textView20"
        android:layout_alignStart="@+id/textView20" />

    <TextView
        android:text="Password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView21"
        android:layout_alignLeft="@+id/textView21"
        android:layout_alignStart="@+id/textView21"
        android:layout_marginTop="50dp"
        android:id="@+id/textView22" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:inputType="textPersonName"
        android:layout_above="@+id/textView19"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/editText1" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:inputType="textPersonName"
        android:layout_above="@+id/textView20"
        android:layout_alignLeft="@+id/editText1"
        android:layout_alignStart="@+id/editText1"
        android:id="@+id/editText2" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:inputType="phone"
        android:ems="10"
        android:layout_below="@+id/editText2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="25dp"
        android:id="@+id/editText3" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:inputType="number"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/editText4" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:inputType="textPassword"
        android:layout_alignTop="@+id/textView22"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/editText5" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:inputType="textPassword"
        android:layout_alignTop="@+id/textView23"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/editText6"
        android:layout_alignLeft="@+id/editText1"
        android:layout_alignStart="@+id/editText1" />

    <TextView
        android:text="Retype Pass"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:id="@+id/textView23"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/editText5"
        android:layout_alignLeft="@+id/textView20"
        android:layout_alignStart="@+id/textView20" />

</RelativeLayout>